    <link name="link2">
        <inertial>
            <mass value="100" />
            <origin xyz="1 2 3" rpy="0 -0 0" />
            <inertia ixx="5" ixy="0.0" ixz="0.0" iyy="15" iyz="0.0" izz="25" />
        </inertial>
